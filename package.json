{
  "name": "isbot",
  "version": "3.0.0",
  "description": "detects bots/crawlers/spiders via the user agent.",
  "keywords": [
    "bot",
    "crawlers",
    "spiders",
    "googlebot",
    "useragent"
  ],
  "author": "Goran Gajic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gorangajic/isbot/issues"
  },
  "homepage": "https://github.com/gorangajic/isbot",
  "repository": {
    "type": "git",
    "url": "https://github.com/gorangajic/isbot"
  },
  "engines": {
    "node": ">=6"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "prepare": "./scripts/download-fixtures.sh",
    "sort": "./scripts/sort.js",
    "unit": "node --experimental-worker ./node_modules/.bin/mocha tests/*.js --exclude 'tests/.*'",
    "prebrowser": "rollup --config .rollup.js",
    "browser": "karma start .karma.js",
    "lint": "standard",
    "ts": "tsc",
    "test": "npm run sort && npm run unit && npm run browser && npm run lint && npm run ts"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.0.3",
    "array-flat-polyfill": "^1.0.1",
    "eslint-plugin-log": "^1.2.3",
    "karma": "^5.0.5",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-firefox-launcher": "^1.3.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "mocha": "^7.1.2",
    "rollup": "^2.10.0",
    "standard": "^14.3.4",
    "stdline": "^0.1.0",
    "typescript": "^3.9.2",
    "user-agents": "^1.0.559",
    "yaml": "^1.9.2"
  }
}
